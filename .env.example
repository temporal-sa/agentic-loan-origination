#Temporal settings
# For local: TEMPORAL_ADDRESS=localhost:7233, TEMPORAL_NAMESPACE=default
# For cloud: TEMPORAL_ADDRESS=your-namespace.account-id.tmprl.cloud:7233, TEMPORAL_NAMESPACE=your-namespace.account-id
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=loan-underwriter-queue

# Optional: Temporal Cloud API Key (if not set, connects without authentication for local)
# TEMPORAL_API_KEY=your-api-key-here
#LLM settings
MODEL_PROVIDER=aws-bedrock # either 'ollama' or 'aws-bedrock'

#Ollama settings
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3:latest

#AWS Bedrock settings
AWS_BEARER_TOKEN_BEDROCK=<apikey>
AWS_REGION=ap-southeast-2
AWS_BEDROCK_MODEL=au.anthropic.claude-sonnet-4-5-20250929-v1:0